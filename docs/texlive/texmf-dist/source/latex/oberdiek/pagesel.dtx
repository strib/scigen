%    \begin{macrocode}
\begingroup\expandafter\expandafter\expandafter\endgroup
\expandafter\ifx\csname currentiflevel\endcsname\relax
  \let\ps@cleanup@if\@empty
\else
  \def\ps@cleanup@if{%
    \ifnum\currentiflevel>\@ne
