\newcommand{\ps@lastpage}{\maxdimen}
\ifx\ps@nofiles\nofiles
  \ifx\ps@testlist\@empty
  \else
    \def\ps@lastpage{0}%
    \newcommand*{\ps@pagetest}[2]{%
      \ifnum#2>\ps@lastpage\relax
        \def\ps@lastpage{#2}%
      \fi
    }%
    \ps@testlist
    \let\ps@pagetest\relax
  \fi
\fi
