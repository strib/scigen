\expandafter\ifx\csname Hv@scale\endcsname\relax
 \let\Hv@@scale\@empty
\else
 \edef\Hv@@scale{s*[\csname Hv@scale\endcsname]}%
\fi
\DeclareFontFamily{TS1}{phv}{}
\DeclareFontShape{TS1}{phv}{m}{n}{
   <-> \Hv@@scale phvr8c
}{}
