}{}
\DeclareFontShape{TS1}{phv}{mc}{n}{
   <-> \Hv@@scale phvr8cn
}{}
\DeclareFontShape{TS1}{phv}{mc}{sl}{
   <-> \Hv@@scale phvro8cn
}{}
\DeclareFontShape{TS1}{phv}{b}{n}{
   <-> \Hv@@scale phvb8c
}{}
\DeclareFontShape{TS1}{phv}{b}{sl}{
   <-> \Hv@@scale phvbo8c
}{}
\DeclareFontShape{TS1}{phv}{bc}{n}{
   <-> \Hv@@scale phvb8cn
}{}
\DeclareFontShape{TS1}{phv}{bc}{sl}{
   <-> \Hv@@scale phvbo8cn
}{}
\DeclareFontShape{TS1}{phv}{m}{it}{<->ssub * phv/m/sl}{}
\DeclareFontShape{TS1}{phv}{mc}{it}{<->ssub * phv/mc/sl}{}
