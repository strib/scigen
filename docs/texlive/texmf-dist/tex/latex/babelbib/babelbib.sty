    }{#2}%
  }%
}
\DeclareRobustCommand\btxnumeralswedish[2]{%
  \foreignlanguage{#1}{%
    \ifnumber{#2}{%
      \expandafter\btx@parsenum#2\@empty\@empty\@empty
      \btxnumeralfont{%
        \ifx\btx@digitb\@empty
          \ifcase\btx@digita :e\or :a\or :a\or :e\else :e\fi
        \else
          \ifnum\btx@digita=1
            :a%
          \else
            \ifcase\btx@digitb :e\or :a\or :a\or :e\else :e\fi
          \fi
        \fi
        }%
    }{#2}%
  }%
}
