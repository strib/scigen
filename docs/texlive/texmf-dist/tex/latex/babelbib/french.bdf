\DeclareRobustCommand\btxnumeralfrench[2]{%
  \foreignlanguage{#1}{%
    \ifnumber{#2}{%
      \expandafter\btx@parsenum#2\@empty\@empty\@empty
      \btxnumeralfont{%
        \ifx\btx@digitb\@empty
          \ifcase\btx@digita \ieme\or \iere\else \ieme\fi
        \else
          \ifnum\btx@digita=1
            th%
          \else
            \ifcase\btx@digitb \ieme\or \iere\else \ieme\fi
          \fi
        \fi
        }%
    }{#2}%
  }%
}
